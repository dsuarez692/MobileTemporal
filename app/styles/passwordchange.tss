"#mainView":{
	layout: 'vertical',
	backgroundColor: '#EEE'
},
"#mainTopBar":{
	width: Ti.UI.FILL,
	height: "40dp",
	backgroundColor: '#E0E0E0',
	layout: 'horizontal'
},
"#menuButton":{
	top: "0dp",
	left: "0dp",
	width: "40dp",
	height: "40dp",
	zIndex : 2
},
"#appTitle":{
	top: "0dp",
	left: "0dp",
	backgroundColor: '#E0E0E0',
	width: Ti.UI.FILL,
	zIndex: 1
},
"#collapsibleButton":{
	right: "0dp",
	top: "0dp",
	width: "40dp",
	height: "40dp",
	zIndex: 2
},
".appTitleLabel":{
	color:'black',
	textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER
},
"#subtitleBar":{
	top: "0dp",
	left: "0dp",
	width: "100%",
	height: "10%",
	backgroundColor: "#1f4e79",
	layout: 'vertical'
},
".subtitleLabel":{
	color: 'white',
	font: {
		fontSize: '14dp'
	},
	left: '20dp',
	top: '5dp'
}
"#content":{
	height: Ti.UI.FILL,
	width: "100%",
	layout:"vertical",
	backgroundColor: 'white'
},
"#form":{
	top: "0dp",
	left: "0dp",
	height: "70%",
	layout:"vertical",
	scrollType: 'vertical'
},
".buttonSimple":{
	bottom: '0dp',
	left: "10%",
	right: "10%",
	height: "50dp"
},
"#continueBtn":{
	backgroundColor: '#1f4e79',
	backgroundSelectedColor: '#3f8fac'
},
"#cancelBtn":{
	backgroundColor: '#FFFFFF',
	backgroundSelectedColor: '#cecece',
	borderColor: '#1f4e79',
	color: 'black'
}
